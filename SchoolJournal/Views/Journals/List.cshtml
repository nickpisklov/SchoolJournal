@model IEnumerable<SchoolJournal.Models.Journal>
<h1>List</h1>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FkClassNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkSchoolYearNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkSubjectNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkTeacherNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FkClassNavigation.Title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FkSchoolYearNavigation.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FkSubjectNavigation.Title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FkTeacherNavigation.Login)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                    @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                    @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                </td>
            </tr>
        }
    </tbody>
</table>
